    <!-- Page Content -->
    <div class="container">



        <!-- Page Header -->
        <div class="row">
            <div class="col-md-12">
                <h1 class="page-header">
                    Debates
                    <small>{{ current_date }}</small>
                </h1>
            </div>
        </div>

        <!-- Debate Topics -->
        {{#each top_debates_list as | current_topic|}}
            {{#ifFirst @index}}
                  <div class="row">
            {{/ifFirst}}



                    <div class="col-sm-3">



                        <div class="debateCard">
                          <div class="wrapper" style="background: url({{ current_topic.topic_image }}) center/cover no-repeat;">
                            <div class="imageOverlay"></div>
                            <div class="header">
                              <div class="date">
                                <span class="day">12</span>
                                <span class="month">Aug</span>
                                <span class="year">2016</span>
                              </div>
                              <ul class="menu-content">
                                <li>
                                  <a href="#" class="fa fa-bookmark-o bookmark_debate" data-debateInfo="{{current_topic._id}}"></a>
                                </li>
                                <li><a href="#" class="fa fa-heart-o"><span>18</span></a></li>
                                <li><a href="#" class="fa fa-comment-o"><span>3</span></a></li>
                              </ul>
                            </div>
                            <div class="data">
                              <div class="content">
                                <span class="author">Jane Doe</span>
                                <h1 class="title"><a href="#">{{ current_topic.topic_name }}</a></h1>
                                <p class="text">{{ current_topic.topic_desc }}</p>
                                <div class="debate_card_buttons">
                                {{#if ../user}}
                                    <div class="col-lg-6 col-sm-12"><a href="#" class="button join_debate" data-debateList = "top" data-debateName = "{{ current_topic.topic_name_short }}">Join Debate</a></div>
                                    <div class="col-lg-6 col-sm-12"><a href="#" class="button spectate_debate" data-debateList = "top" data-debateName = "{{ current_topic.topic_name_short }}">Spectate</a></div>

                                {{else}}
                                  <a href="#" class="button">Login to Join</a>
                                {{/if}}
                              </div>
                              </div>
                            </div>
                          </div>
                        </div>






















                    </div>


            {{#ifFourth @index}}

                  </div>

            {{else}}

                {{#if @last}}
                    </div>
                {{/if}}

            {{/ifFourth}}


        {{/each}}



        <!-- Pagination -->
        <div class="row text-center">
            <div class="col-lg-12">
                <ul class="pagination">
                    <li>
                        <a href="#">&laquo;</a>
                    </li>
                    <li class="active">
                        <a href="#">1</a>
                    </li>
                    <li>
                        <a href="#">2</a>
                    </li>
                    <li>
                        <a href="#">3</a>
                    </li>
                    <li>
                        <a href="#">4</a>
                    </li>
                    <li>
                        <a href="#">5</a>
                    </li>
                    <li>
                        <a href="#">&raquo;</a>
                    </li>
                </ul>
            </div>
        </div>
        <!-- /.row -->


        <!--User debates-->

        <!-- Page Header -->
        <div class="row">
            <div class="col-md-12">
                <h1 class="page-header">
                    User Debates
                    <small>{{ current_date }}</small>
                </h1>
            </div>
        </div>

        <!-- user Debate Topics -->
        {{#each user_debates_list as | current_topic|}}
            {{#ifFirst @index}}
                  <div class="row">
            {{/ifFirst}}



                    <div class="col-sm-4">
                        <div class="card">
                            <div class="card-image">
                                <img class="img-responsive" src="{{ current_topic.topic_image }}" alt="">
                                <span class="card-title">
                                {{ current_topic.topic_name }}
                                <p class="card-desc">{{ current_topic.topic_desc }}</p>
                                </span>

                            </div>

                            <!--<div class="card-content">
                                <p>{{ current_topic.topic_desc }}</p>
                            </div>-->

                            <div class="card-action" align="center">
                                  {{#if ../user}}
                        <button class="btn btn-default join_debate debate_button" data-debateList = "users" data-debateName = "{{ current_topic.topic_name_short }}">Join Debate</button>
                        <button class="btn btn-default spectate_debate debate_button" data-debateList = "users" data-debateName = "{{ current_topic.topic_name_short }}">Spectate Debate</button>
                       {{else}}
                        <a href="/users/login"><button class="btn btn-default" >Login to Join Debate</button></a>
                        <button class="btn btn-default spectate_debate debate_button" data-debateList = "users" data-debateName = "{{ current_topic.topic_name_short }}">Spectate Debate</button>
                       {{/if}}


                            </div>
                        </div>
                    </div>


            {{#ifFourth @index}}

                  </div>

            {{else}}

                {{#if @last}}
                    </div>
                {{/if}}

            {{/ifFourth}}


        {{/each}}


        <!-- Pagination -->
        <div class="row text-center">
            <div class="col-lg-12">
                <ul class="pagination">
                    <li>
                        <a href="#">&laquo;</a>
                    </li>
                    <li class="active">
                        <a href="#">1</a>
                    </li>
                    <li>
                        <a href="#">2</a>
                    </li>
                    <li>
                        <a href="#">3</a>
                    </li>
                    <li>
                        <a href="#">4</a>
                    </li>
                    <li>
                        <a href="#">5</a>
                    </li>
                    <li>
                        <a href="#">&raquo;</a>
                    </li>
                </ul>
            </div>
        </div>
        <!-- /.row -->

        <hr>



    </div>
    <!-- /.container -->
